
def get_money_spent(keyboards, drives, b)
  #
  # Write your code here                                                                                                                                                  ffffffffffffffffffffffffffffffffffffffffffffffff
  #

  arr = []}{
  final = [
  keyoards.each do |i| 
    drives.each do |j|
      arr.push(i + j)
